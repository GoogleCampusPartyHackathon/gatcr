{
  "name": "Google Analytics Tracking Cookie Reader",
  "description": "Chrome extension, which displays information saved on your computer by Google Analytics.",
  "version": "0.1",
  "manifest_version": 2,
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/19.png", // optional
    "default_title": "Show data in tracking cookie",    // optional; shown in tooltip
    "default_popup": "src/popup.html"    // optional
  },
  "background": {
  },
  "permissions": [
    "cookies",
    "tabs",
    "*://*/*"
  ]
}
